{
  "schemaVersion": 1,
  "id": "kambrik",
  "version": "${version}",

  "name": "Kambrik",
  "description": "A Light-weight Kotlin Library Mod!",
  "authors": [
    "Ejektaflex"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/kambrik",
    "sources": "https://github.com/ejektaflex/Kambrik",
    "issues": "https://github.com/ejektaflex/Kambrik/issues"
  },
  "license": "GPL 3.0",
  "icon": "assets/kambrik/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
		"io.ejekta.kambrik.KambrikModFabric"
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "io.ejekta.kambrik.internal.client.KambrikClientMod"
      }
    ]
  },
  "mixins": [
	  "kambrik.mixins.json"
  ],
  "custom": {
    "modmenu": {
      "badges": [ "library" ]
    }
  },
  "depends": {
    "fabric": ">=0.60.0",
    "minecraft": "~1.19.4-0",
    "fabric-language-kotlin": ">=1.8.10"
  }
}